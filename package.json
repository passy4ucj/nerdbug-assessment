{
  "name": "nerdbug",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "start": "NODE_ENV=production tsc && nodemon ./src/index.ts",
    "startup": "cross-env NODE_ENV=production tsc && sequelize db:create && sequelize db:migrate && sequelize db:seed:all && nodemon ./src/index.ts",
    "db-migrate": "tsc && sequelize db:migrate",
    "db-migrate2": "tsc && sequelize db:migrate && sequelize db:seed:all",
    "build": "tsc -p . ",
    "db-seed": "sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test && jest --detectOpenHandles",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:create:test": "cross-env NODE_ENV=test sequelize db:create",
    "db:reset": "tsc && sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "author": "Pascal Ojinnaka",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0",
    "redis": "^4.6.10",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "supertest": "^6.3.3",
    "uuidv4": "^6.2.13",
    "winston": "^3.10.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.5.9",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.11.1",
    "cross-env": "^7.0.3",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize-cli": "^6.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
